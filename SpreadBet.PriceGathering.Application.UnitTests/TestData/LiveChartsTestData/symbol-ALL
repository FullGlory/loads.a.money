    <div align="left" style="margin-bottom: 5px;">
                        		<!--<script src="js/prototype/prototype.js"></script>-->
		<script src="http://www.livecharts.co.uk/share_prices/js/prototype/prototype.js"></script>
		<script type="text/javascript">
		function requestIndexesOrAZ(fndNumber) {
			if ( $('combo_security_type').value == "share" ) {
				$('container_az').style.display='';
				$('container_indexes').style.display='none';
				$('container_shares').style.display='none';
				requestAZ();
			}
			else if ( $('combo_security_type').value == "index" ) {
				$('container_az').style.display='none';
				$('container_indexes').style.display='';
				$('container_shares').style.display='none';
				requestIndexes();
			}
		}

		function requestIndexes() {
			new Ajax.Updater(
				'combo_indexes',

				'http://www.livecharts.co.uk/share_prices/symbol_finder_ajax.php',

				{
			  method: 'get',
			  parameters: { toLoad : "indexes"}
			  }

			  );
		}
		function requestAZ() {
			new Ajax.Updater(
				'combo_az',

				'http://www.livecharts.co.uk/share_prices/symbol_finder_ajax.php',

				{
			  method: 'GET',
			  parameters: { toLoad : "az"}
			  }

			  );
		}
		function requestShares() {
			if ( $('combo_az').value == "" ) {
				return;
			}
			$('container_shares').style.display='';
			new Ajax.Updater(
				'combo_shares',

				'http://www.livecharts.co.uk/share_prices/symbol_finder_ajax.php',

				{
			  method: 'get',
			  parameters: { toLoad : "shares", letter : $('combo_az').value}
			  }

			  );
		}

		function goToSharePrice(){
			document.location='http://www.livecharts.co.uk/share_prices/share_price/symbol-' + $('symbol').value;
			return false;
		}
		function changeIndexHandler() {
							document.location='http://www.livecharts.co.uk/share_prices/share_price/symbol-' + $('combo_indexes').value;
						}
		function changeShareHandler() {
							document.location='http://www.livecharts.co.uk/share_prices/share_price/symbol-' + $('combo_shares').value;
						}
		function enterSymbolHandler() {
							document.location='http://www.livecharts.co.uk/share_prices/share_price/symbol-' + $('symbol').value.toUpperCase();
							return false;
		}

		</script>
				<table bgcolor="#a2a2a2" cellpadding="0" cellspacing="0" border="0" style="height: 38px; width:300px;border:1px solid #000000;padding-top:3px; margin-bottom:5px;">
					<tr>
						<td style="padding-left:10px;padding-bottom:3px;">
						<form method="GET" onsubmit="return enterSymbolHandler()">
								<input type="text" id="symbol" name="symbol" style="width:90px;" value="  Insert ticker" onclick="this.value=''" />
								<input type="submit" value="Go"/>
						</form></td><td> <span style="font-size:1.1em;color: #FFFFFF;font-weight: bold;">Enter share symbol</span>
						</td>
					</tr>
				</table>

